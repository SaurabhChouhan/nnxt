    "start.gaurav.local": "cross-env BABEL_ENV=server NODE_ENV=gaurav_local nodemon ./server/server.js --exec babel-node",
    "start.murtaza": "cross-env BABEL_ENV=server NODE_ENV=murtaza nodemon ./server/server.js --exec babel-node",
