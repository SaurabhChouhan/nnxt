    "start.anup": "cross-env BABEL_ENV=server NODE_ENV=anup nodemon ./server/server.js --exec babel-node",
    "start.ravi": "cross-env BABEL_ENV=server NODE_ENV=ravi nodemon ./server/server.js --exec babel-node",
    "start.ratnesh": "cross-env BABEL_ENV=server NODE_ENV=ratnesh nodemon ./server/server.js --exec babel-node",
